        "expect.js": "^0.3.1",
        "grunt": "^1.0.1",
        "grunt-cli": "^1.1.0",
        "grunt-contrib-watch": "^1.0.0",
        "grunt-eslint": "^18.1.0",
        "grunt-exec": "^0.4.7",
        "grunt-simple-mocha": "^0.4.1",
        "husky": "^0.14.3",
        "in-publish": "^2.0.0",
        "istanbul": "^0.4.3",
        "lint-staged": "^7.0.0",
        "load-grunt-tasks": "^3.5.0",
        "mocha": "^2.5.3",
        "multiline": "^1.0.2",
        "nock": "^9.2.3",
        "node-uuid": "^1.4.7",
        "prettier": "^1.11.1",
        "proxyquire": "^1.7.9",
        "spawn-sync": "1.0.15",
        "wrench": "^1.5.8",
        "babel-cli": "latest",
        "babel-core": "latest",
        "babel-plugin-add-module-exports": "latest",
        "babel-plugin-transform-es2015-modules-commonjs": "latest",
        "babel-polyfill": "latest",
        "babel-plugin-transform-object-rest-spread": "latest",
        "babel-preset-env": "latest"
    },
    "scripts": {
        "test": "grunt test",
        "ci": "grunt travis",
        "coveralls": "coveralls",
        "prepublishOnly": "in-publish && echo 'You need to use \"grunt publish\" to publish bower' && false || not-in-publish",
        "format": "prettier --write --single-quote --tab-width 4 '**/*.js'",
        "precommit": "lint-staged",
        "compile-lib": "babel lib --out-dir lib --require babel-polyfill --presets env",
        "compile-test": "babel test --out-dir test --require babel-polyfill --presets env",
        "compile-libutil": "babel lib\\util --out-dir lib\\util --require babel-polyfill --presets env",
        "compile-libbin": "babel lib\\bin --out-dir lib\\bin --require babel-polyfill --presets env",
        "compile-testutil": "babel test\\util --out-dir test\\util --require babel-polyfill --presets env",
        "compile-libcore": "babel lib\\core --out-dir lib\\core --require babel-polyfill --presets env",
        "compile-libcommands": "babel lib\\commands --out-dir lib\\commands --require babel-polyfill --presets env",
        "compile-testcore": "babel test\\core --out-dir test\\core --require babel-polyfill --presets env",
        "compile-testcommands": "babel test\\commands --out-dir test\\commands --require babel-polyfill --presets env",
        "compile-librenderers": "babel lib\\renderers --out-dir lib\\renderers --require babel-polyfill --presets env",
        "compile-libcommandscache": "babel lib\\commands\\cache --out-dir lib\\commands\\cache --require babel-polyfill --presets env",
        "compile-libcoreresolvers": "babel lib\\core\\resolvers --out-dir lib\\core\\resolvers --require babel-polyfill --presets env",
        "compile-testcommandscache": "babel test\\commands\\cache --out-dir test\\commands\\cache --require babel-polyfill --presets env",
        "compile-libtemplateshelpers": "babel lib\\templates\\helpers --out-dir lib\\templates\\helpers --require babel-polyfill --presets env",
        "compile-testrenderers": "babel test\\renderers --out-dir test\\renderers --require babel-polyfill --presets env",
        "compile-testcoreresolvers": "babel test\\core\\resolvers --out-dir test\\core\\resolvers --require babel-polyfill --presets env",
        "compile-testassetstesttempdir": "babel test\\assets\\test-temp-dir --out-dir test\\assets\\test-temp-dir --require babel-polyfill --presets env",
        "compile-testassetspackageatest": "babel test\\assets\\package-a\\test --out-dir test\\assets\\package-a\\test --require babel-polyfill --presets env",
        "compile-1_script": "npm run compile-lib && npm run compile-test && npm run compile-libutil && npm run compile-libbin && npm run compile-testutil && npm run compile-libcore && npm run compile-libcommands && npm run compile-testcore && npm run compile-testcommands && npm run compile-librenderers && npm run compile-libcommandscache && npm run compile-libcoreresolvers && npm run compile-testcommandscache && npm run compile-libtemplateshelpers && npm run compile-testrenderers && npm run compile-testcoreresolvers && npm run compile-testassetstesttempdir && npm run compile-testassetspackageatest",
        "compile": "npm run compile-1_script"
    },
    "lint-staged": {
        "*.js": [
            "prettier --single-quote --tab-width 4",
            "git add"
        ]
    },
    "files": [
        "bin",
        "lib"
    ]
}