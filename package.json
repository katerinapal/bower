{
    "name": "bower",
    "version": "1.8.8",
    "description": "The browser package manager",
    "author": "Twitter",
    "license": "MIT",
    "repository": "bower/bower",
    "main": "lib",
    "bin": "bin/bower",
    "homepage": "http://bower.io",
    "engines": {
        "node": ">=0.10.0"
    },
    "keywords": [
        "bower"
    ],
    "dependencies": {
